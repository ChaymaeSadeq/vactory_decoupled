<div>
  {% for item in content %}
    <div>
      {% if item.title is not empty %}
        <h3>{{ item.title }}</h3>
      {% endif %}
      {% if item.description is not empty %}
        <p>
          {{ item.description }}
        </p>
      {% endif %}
      {% if item.link.title is not empty and item.link.url is not empty %}
        <a href="{{ item.link.url }}">{{ item.link.title }}</a>
      {% endif %}
    </div>

  {% endfor %}
</div>
