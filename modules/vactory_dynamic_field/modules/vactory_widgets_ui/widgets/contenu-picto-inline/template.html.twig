<div>
  {% if extra_fields.bigTitle is not empty %}
    <h2>{{ extra_fields.bigTitle }}</h2>
  {% endif %}
  {% if extra_fields.intro is not empty %}
    <p>{{ extra_fields.intro }}</p>
  {% endif %}

  <div>
    {% for item in content %}
      <div>
        {% if item.title is not empty %}
          <h3>{{ item.title }}</h3>
        {% endif %}
        <div>
          {{ item.description.value|raw }}
        </div>
      </div>

    {% endfor %}
  </div>
</div>
